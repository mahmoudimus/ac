<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="viewport" content="width=device-width">
  <meta name="ap-local-base-url" content="$localBaseUrl">
  #if ($authState)
  <meta name="ap-auth-state" content="$authState">
  #end
  #if ($pageTitle)
  <title>$pageTitle</title>
  #end
  #parse ($headTemplate)
  #foreach ($url in $appStylesheetUrls)
  <link rel="stylesheet" href="$url">
  #end
  <script src="$hostScriptUrl" data-options="$!clientOptions"></script>
  <!--  This intialization is no longer required, but kept around for older systems. -->
  <script>if (!window.AP) RA.init();</script>
</head>
<body>
  $bodyHtml
  #parse ($tailTemplate)
  #if ($bigPipeActivated)
  <script>
    if (window.AP) {
      AP.BigPipe.start({
        requestId: "$bigPipeRequestId"
      });
    }
  </script>
  #end
  #foreach ($url in $appScriptUrls)
  <script src="$url"></script>
  #end
</body>
</html>
