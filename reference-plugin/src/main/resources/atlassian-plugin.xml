<atlassian-plugin key="${project.groupId}.${project.artifactId}" name="${project.name}" plugins-version="2">
    <plugin-info>
        <description>${project.description}</description>
        <version>${project.version}</version>
        <vendor name="${project.organization.name}" url="${project.organization.url}"/>
    </plugin-info>

    <connect-context-parameters-validator key="test-parameters-validator" class="com.atlassian.plugin.connect.TestContextParametersValidator" />
    <connect-context-parameters-extractor key="test-parameters-extractor" class="com.atlassian.plugin.connect.TestContextParametersExtractor" />

</atlassian-plugin>
