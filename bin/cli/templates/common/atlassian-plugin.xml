<?xml version="1.0" ?>
<atlassian-plugin key="@app_key@" name="@app_name@" plugins-version="2">

    <plugin-info>
        <description>@app_desc@</description>
        <version>1</version>
        <permissions>
            @app_permissions@
        </permissions>
    </plugin-info>

    <remote-plugin-container key="container" display-url="http://localhost:8000/@app_key@">
        @app_oauth_section@
    </remote-plugin-container>

    <general-page key="general" name="@app_name@ General Page" url="/general" />

</atlassian-plugin>
