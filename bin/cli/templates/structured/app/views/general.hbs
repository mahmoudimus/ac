<h1>Request Info</h1>
<table width="100%">
  <tr>
    <th align="left" colspan="2">headers</th>
  </tr>
  {{#each headers}}
  <tr>
    <td>{{name}}</td>
    <td>{{value}}</td>
  </tr>
  {{else}}
  <tr>
    <td align="left" colspan="2">(no headers)</td>
  </tr>
  {{/each}}
  <tr>
    <th align="left" colspan="2">path params</th>
  </tr>
  {{#each pathParams}}
  <tr>
    <td>{{name}}</td>
    <td>{{value}}</td>
  </tr>
  {{else}}
  <tr>
    <td align="left" colspan="2">(no path params)</td>
  </tr>
  {{/each}}
  <tr>
    <th align="left" colspan="2">query params</th>
  </tr>
  {{#each queryParams}}
  <tr>
    <td>{{name}}</td>
    <td>{{value}}</td>
  </tr>
  {{else}}
  <tr>
    <td align="left" colspan="2">(no query params)</td>
  </tr>
  {{/each}}
  <tr>
    <th align="left" colspan="2">other properties</th>
  </tr>
  <tr>
    <td>query string</td>
    <td>{{queryString}}</td>
  </tr>
  <tr>
    <td>host base url</td>
    <td>{{hostBaseUrl}}</td>
  </tr>
  <tr>
    <td>client key</td>
    <td>{{clientKey}}</td>
  </tr>
  <tr>
    <td>user id</td>
    <td>{{userId}}</td>
  </tr>
  <tr>
    <td>method</td>
    <td>{{method}}</td>
  </tr>
  <tr>
    <td>script name</td>
    <td>{{scriptName}}</td>
  </tr>
  <tr>
    <td>path info</td>
    <td>{{pathInfo}}</td>
  </tr>
  <tr>
    <td>host</td>
    <td>{{host}}</td>
  </tr>
  <tr>
    <td>port</td>
    <td>{{port}}</td>
  </tr>
  <tr>
    <td>scheme</td>
    <td>{{scheme}}</td>
  </tr>
</table>
