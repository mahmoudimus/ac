<div class="ap-container" id="ap-$namespace">

  <div class="ap-content" id="embedded-$namespace"></div>
  <div class="ap-stats hidden">
    <div class="ap-loading ap-status hidden">
      <small><div class="small-spinner"></div>Loading add-on <a href="${plugin.baseUrl}" class="ap-doc-url" target="_blank">$plugin.name</a>.</small>
    </div>
    <div class="ap-load-timeout ap-status hidden">
      <small><div class="small-spinner"></div>Add-on <a href="${plugin.baseUrl}" class="ap-doc-url" target="_blank">$plugin.name</a> is not responding.
      Wait or <a href="#" class="ap-btn-cancel">cancel</a>?</small>
    </div>
    <div class="ap-load-error ap-status hidden">
      <small>Add-on <a href="${plugin.baseUrl}" class="ap-doc-url" target="_blank">$plugin.name</a> failed to load.</small>
    </div>
  </div>

  #parse("velocity/iframe-body-script.vm")

</div>
