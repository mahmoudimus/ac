<atlassian-plugin key="test" name="Remoteapp test plugin" plugins-version="2">
    <plugin-info>
        <version>1</version>
    </plugin-info>

    <rest key="rest" path="/remoteapptest" version="1">
        <package>com.atlassian.labs.remoteapps.test.rest</package>
    </rest>
    <rest key="forbiddenrest" path="/remoteappforbidden" version="1">
        <package>com.atlassian.labs.remoteapps.test.forbiddenrest</package>
    </rest>
    <component-import key="userManager" interface="com.atlassian.sal.api.user.UserManager" />

    <api-scope key="resttest" name="Rest Test" class="com.atlassian.labs.remoteapps.test.RestTestApiScope">
        <description>A test resource</description>
    </api-scope>

</atlassian-plugin>
