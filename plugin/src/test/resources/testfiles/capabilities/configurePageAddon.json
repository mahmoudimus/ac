{
    "key": "my-add-on",
    "name": "My Add-On",
    "version": "2.0",
    "vendor": {
        "name": "Atlassian",
        "url": "http://www.atlassian.com"
    },
    "baseUrl": "http://www.example.com",
    "authentication": {
        "type": "jwt"
    },
    "modules": {
        "configurePages": [
            {
                "isDefault": false,
                "url": "/my-page",
                "icon": {
                    "width": 80,
                    "height": 80,
                    "url": "/mypage/icon.png"
                },
                "name": {
                    "value": "Some page",
                    "i18n": "some.page.name"
                }
            },
            {
                "isDefault": true,
                "url": "/my-page2",
                "icon": {
                    "width": 80,
                    "height": 80,
                    "url": "/mypage2/icon.png"
                },
                "name": {
                    "value": "Some otro page",
                    "i18n": "some.page2.name"
                }
            }
        ]
    }}
