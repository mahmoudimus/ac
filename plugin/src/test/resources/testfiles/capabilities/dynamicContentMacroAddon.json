{
  "authentication": {
    "type": "jwt"
  },
  "baseUrl": "http://www.example.com",
  "key": "my-add-on",
  "modules": {
    "dynamicContentMacros": [
      {
        "aliases": [
          "some-alias"
        ],
        "bodyType": "plain-text",
        "categories": [
          "hidden-macros"
        ],
        "description": {
          "i18n": "some.macro.desc",
          "value": "Some Description"
        },
        "documentation": {
          "altText": "Doc Alt Text",
          "title": "Doc Title",
          "url": "http://docs.example.com/macro"
        },
        "featured": true,
        "height": 50,
        "icon": {
          "height": 80,
          "url": "/mymacro/icon.png",
          "width": 80
        },
        "name": {
          "i18n": "some.macro.name",
          "value": "Some Macro"
        },
        "outputType": "block",
        "parameters": [
          {
            "aliases": [
              "paramAlias1"
            ],
            "defaultValue": "default",
            "description": {
              "i18n": "desc.key",
              "value": "Some Description"
            },
            "identifier": "myparam",
            "multiple": false,
            "name": {
              "i18n": "name.key",
              "value": "Some Name"
            },
            "required": true,
            "type": "enum",
            "values": [
              "paramValue1",
              "paramValue2"
            ]
          }
        ],
        "url": "/my-macro",
        "width": 100
      }
    ]
  },
  "name": "My Add-On",
  "vendor": {
    "name": "Atlassian",
    "url": "http://www.atlassian.com"
  },
  "version": "2.0"
}
