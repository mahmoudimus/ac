<atlassian-plugin key="${project.groupId}.${project.artifactId}" name="${project.name}" plugins-version="2">
    <plugin-info>
        <description>${project.description}</description>
        <version>${project.version}</version>
        <vendor name="${project.organization.name}" url="${project.organization.url}"/>
    </plugin-info>

    <connect-context-parameters-resolver key="test-connect-context-parameters-resolver">
        <extractors>
            <extractor class="com.atlassian.plugin.connect.TestJiraContextParametersExtractor"/>
        </extractors>
        <validators>
            <validator class="com.atlassian.plugin.connect.TestJiraContextParametersValidator"/>
        </validators>
    </connect-context-parameters-resolver>
    
    <component-import key="iFrameRenderStrategyBuilderFactory" interface="com.atlassian.plugin.connect.api.iframe.render.strategy.IFrameRenderStrategyBuilderFactory"/>
    <component-import key="iFrameRenderStrategyRegistry" interface="com.atlassian.plugin.connect.api.iframe.render.strategy.IFrameRenderStrategyRegistry"/>
    <component-import key="webItemModuleDescriptorFactory" interface="com.atlassian.plugin.connect.spi.capabilities.descriptor.WebItemModuleDescriptorFactory"/>
    <component-import key="pageConditionsFactory" interface="com.atlassian.plugin.connect.spi.condition.PageConditionsFactory"/>

    <connect-module
            key="testJiraModules"
            class="com.atlassian.plugin.connect.JiraModuleProviderForTests"
            name="Test Jira Module Provider">
        <description>...</description>
    </connect-module>

</atlassian-plugin>
