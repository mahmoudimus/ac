<div class="aui-nav-heading">
    <% if (modules.index) { %>
        <a href="<%= baseUrl %>/<%= modules.index.selfLink %>.html"><%= modules.index.name %></a>
    <% } else { %>
        <strong><%= title %></strong>
    <% } %>
</div>
<ul class="aui-nav">
    <% for (var key in modules) { %>
        <% if (key === "index") continue; %>
        <% var module = modules[key]; %>
        <% var selected = module.selfLink === current.path.join("/") ? "class=aui-nav-selected" : ""; %>
        <li <%= selected %>>
            <a href="<%= baseUrl %>/<%= module.selfLink %>.html"><%= module.name %></a>
        </li>
    <% } %>
</ul>
